        <style>
            .banner_new_area {
                background: linear-gradient(rgba(52, 152, 219, 0.85), rgba(52, 152, 219, 0.85)), url(img/home-slider/home-two-bg.jpg) no-repeat scroll center;
                background-size: cover;


            }

            .banner_new_layer {
                content: "";
                width: 100%;
                height: 100%;
                top: 0;
                left: 0;
                position: absolute;
                background: #3498db;
                opacity: 0.95;
            }

            .banner_new_area .b_new_text {
                padding-top: 130px;
            }

            .banner_new_area .b_new_text h3 {
                font-size: 60px;
                color: #fff;
                text-transform: uppercase;
                font-family: "Heebo", sans-serif;
            }

            .banner_new_area .b_new_text h3 span {
                color: #000;
            }

            .banner_new_area .b_new_text h5 {
                font-family: "Heebo", sans-serif;
                color: #fff;
                font-size: 20px;
                text-transform: uppercase;
                letter-spacing: 10px;
                padding: 45px 0px 80px 0px;
            }

            .banner_new_area .b_new_image {
                text-align: center;
            }

            .banner_new_area .b_new_image img {
                max-width: 100%;
            }

        </style>
